CREATE TABLE Users (
	cpf char(11) PRIMARY KEY, 
	name varchar(50),
	email varchar(100) UNIQUE,
	password varchar(255),
	accessToken varchar(255)
);


CREATE TABLE AccessToken(
	id VARCHAR(255) PRIMARY KEY,
	expiresIn INT,
	userCPF char(11),
	FOREIGN KEY (userCpf) REFERENCES Users(cpf)
	ON UPDATE CASCADE ON DELETE SET NULL
)

ALTER TABLE USERS ADD FOREIGN KEY (accessToken) REFERENCES ACCESSTOKEN (id) ON UPDATE CASCADE ON DELETE SET DEFAULT;

CREATE TABLE Employee(
	cpf varchar(11) PRIMARY KEY,
	name varchar(150),
	phone varchar(15),
	salary float,
	profession varchar(50)
)

CREATE TABLE Costumer(
	cpf varchar(11) PRIMARY KEY,
	name varchar(150),
	address varchar(255),
	phone varchar(15),
);
ALTER TABLE COSTUMER ADD FOREIGN KEY (animalsCode) REFERENCES Animal (id) ON UPDATE CASCADE ON DELETE SET DEFAULT;


CREATE TABLE Animal(
	id  SERIAL PRIMARY KEY,
	name varchar(100),
	species varchar(100),
	breed varchar(100),
	FOREIGN KEY (cpf_costumer) REFERENCES Costumer(cpf)
)

